         <content>
            <FlowPane fx:id="goodsView" prefHeight="720.0" prefWidth="1000.0" />
         </content>
      </ScrollPane>
      <TextField fx:id="findText" layoutX="206.0" layoutY="24.0" prefHeight="50.0" prefWidth="535.0" />
      <Button fx:id="refresh" layoutX="878.0" layoutY="24.0" mnemonicParsing="false" onAction="#findGoodsOnClick" prefHeight="50.0" prefWidth="100.0" text="刷新" />
