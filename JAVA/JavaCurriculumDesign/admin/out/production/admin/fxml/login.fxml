<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" stylesheets="@../css/login.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminApplication.Controllers.loginController">
   <children>
      <ImageView fx:id="loginIcon" fitHeight="250.0" fitWidth="400.0" layoutX="27.0" layoutY="113.0" pickOnBounds="true" preserveRatio="true">
      </ImageView>
      <AnchorPane layoutX="421.0" layoutY="44.0" prefHeight="414.0" prefWidth="317.0">
         <children>
            <Label layoutX="24.0" layoutY="65.0" prefHeight="53.0" prefWidth="77.0" text="账号 :" />
            <TextField fx:id="userId" layoutX="101.0" layoutY="70.0" prefHeight="43.0" prefWidth="202.0" />
            <Label accessibleRole="PASSWORD_FIELD" layoutX="24.0" layoutY="127.0" prefHeight="53.0" prefWidth="77.0" text="密码 :" />
            <PasswordField fx:id="password" layoutX="101.0" layoutY="127.0" prefHeight="43.0" prefWidth="202.0" />
            <Button fx:id="loginIn" layoutX="101.0" layoutY="220.0" mnemonicParsing="false" onAction="#loginInOnClick" prefHeight="61.0" prefWidth="163.0" text="登陆" />
            <Label fx:id="loginMessage" layoutX="48.0" layoutY="24.0" prefHeight="30.0" prefWidth="221.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
