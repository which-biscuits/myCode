<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminApplication.Controllers.goodsInfoController">
   <children>
      <ScrollPane layoutY="100.0" prefHeight="650.0" prefWidth="1500.0">
         <content>
            <TableView fx:id="goodsInfo" layoutX="10.0" prefHeight="645.0" prefWidth="1490.0">
              <columns>
                <TableColumn fx:id="goodsId" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="250.0" text="商品ID" />
                <TableColumn fx:id="goodsName" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="商品名称" />
                  <TableColumn fx:id="introduction" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="商品介绍" />
                  <TableColumn fx:id="goodsPrice" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="商品价格" />
                  <TableColumn fx:id="keyWords" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="关键词" />
                  <TableColumn fx:id="orCheck" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="90.0" text="审核状态" />
                  <TableColumn fx:id="failReason" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="125.0" text="审核失败原因" />
                  <TableColumn fx:id="saleOut" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="是否卖出" />
                  <TableColumn fx:id="outTime" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="125.0" text="卖出时间" />
                  <TableColumn fx:id="purchaserId" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="购买人" />
                  <TableColumn fx:id="receiveAddress" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="收货地址" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
      <Label alignment="CENTER" layoutX="20.0" layoutY="20.0" prefHeight="60.0" prefWidth="200.0" styleClass="titleLabel" text="商品信息" />
      <Button fx:id="deleteGoods" layoutX="726.0" layoutY="30.0" mnemonicParsing="false" onAction="#deleteGoodsOnClick" prefHeight="40.0" prefWidth="100.0" text="删除商品" />
      <TextField fx:id="goodsIdTextIn" layoutX="488.0" layoutY="30.0" prefHeight="40.0" prefWidth="200.0" />
      <Button fx:id="auditGoods" layoutX="863.0" layoutY="30.0" mnemonicParsing="false" onAction="#auditGoodsOnClick" prefHeight="40.0" prefWidth="100.0" text="审核商品" />
      <Label alignment="CENTER" layoutX="388.0" layoutY="30.0" prefHeight="40.0" prefWidth="100.0" text="商品ID : " />
   </children>
</AnchorPane>
