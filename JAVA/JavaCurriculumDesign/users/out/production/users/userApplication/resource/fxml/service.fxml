<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="550.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userApplication.Controllers.serviceController">
   <children>
      <Label alignment="CENTER" prefHeight="60.0" prefWidth="1000.0" styleClass="titleLabel" text="平台客服" />
      <ScrollPane fx:id="serviceScrollPane" hbarPolicy="NEVER" layoutY="55.0" prefHeight="433.0" prefWidth="1000.0">
         <content>
            <VBox fx:id="serviceView" prefHeight="431.0" prefWidth="995.0" />
         </content>
      </ScrollPane>
      <TextField fx:id="textIn" layoutY="487.0" prefHeight="64.0" prefWidth="598.0" />
      <Button fx:id="send" layoutX="598.0" layoutY="487.0" mnemonicParsing="false" onAction="#sendOnClick" prefHeight="64.0" prefWidth="203.0" text="发送" />
      <Button fx:id="sendReadingGoods" layoutX="801.0" layoutY="487.0" mnemonicParsing="false" onAction="#sendReadingGoodsOnClick" prefHeight="64.0" prefWidth="199.0" text="发送正在浏览的商品" />
   </children>
</AnchorPane>
