<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userApplication.Controllers.changePasswordController">
   <children>
      <Button fx:id="confirm" layoutX="160.0" layoutY="295.0" mnemonicParsing="false" onAction="#confirmOnClick" prefHeight="40.0" prefWidth="100.0" text="确认" />
      <Button fx:id="cancel" layoutX="340.0" layoutY="295.0" mnemonicParsing="false" onAction="#cancelOnClick" prefHeight="40.0" prefWidth="100.0" text="取消" />
      <Label layoutX="140.0" layoutY="94.0" prefHeight="40.0" prefWidth="110.0" text="请输入原密码:" />
      <Label layoutX="140.0" layoutY="154.0" prefHeight="40.0" prefWidth="110.0" text="请输入新密码:" />
      <Label layoutX="140.0" layoutY="214.0" prefHeight="40.0" prefWidth="110.0" text="请再次输入:" />
      <TextField fx:id="oldPassword" layoutX="260.0" layoutY="94.0" prefHeight="40.0" prefWidth="200.0" />
      <PasswordField fx:id="firstPassword" layoutX="260.0" layoutY="154.0" prefHeight="40.0" prefWidth="200.0" />
      <Label fx:id="errorMessage" layoutX="200.0" layoutY="33.0" prefHeight="40.0" prefWidth="200.0" />
      <PasswordField fx:id="secondPassword" layoutX="260.0" layoutY="214.0" prefHeight="40.0" prefWidth="200.0" />
   </children>
</AnchorPane>
